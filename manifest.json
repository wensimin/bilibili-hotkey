{
  "manifest_version": 2,

  "name": "bilibili-hotkey",
  "description": "b站播放器快捷键设置",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://www.bilibili.com/video/*"],
      "js": ["jquery-3.2.1.min.js","bilibili-hotkey.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}